{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "xliminatingTrends",
    "description": "Remove the brainwashing trending topics element from your page!",
    "author": "lkszand",
    "browser_action":{
        "default_title": "bettweeter"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
      {
      "matches": ["https://twitter.com/*"],
      "css": ["main.css"]
      }
    ],
    "icons": {
      "16": "logo16.png",
      "32": "logo32.png",
      "64": "logo64.png"
    }
}